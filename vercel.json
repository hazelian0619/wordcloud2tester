{
  "version": 2,
  "builds": [
    { "src": "src/wordcloud_emergence/path-expand.py", "use": "@vercel/python" },
    { "src": "frontend/index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/path-expand", "dest": "/src/wordcloud_emergence/path-expand.py" },
    { "src": "/(.*)", "dest": "/frontend/index.html" }
  ]
}
